{% extends 'base.html.twig' %}

{% block body %}
    <h3>Produtos</h3>
    <div class="row">
        {% for produto in produtos  %}
                <div class="col s12 m12 l4">
                    <div class="card">
                        <div class="card-image">
                            {% if produto.img %}
                                <img class="imagem-card" src="/images/{{ produto.img }}">
                            {% else %}
                                {#<div class="imagem-card">#}
                                    <img class="imagem-card" src="data:image/jpeg;base64,">
                                {#</div>#}
                            {% endif %}
                            <span class="card-title card-titulo">{{ produto.nome }} (R$ {{ produto.precoVenda }})</span>
                        </div>
                        <div class="card-content">
                            <p>{{ produto.descricao|striptags|length > 30 ? produto.descricao|striptags|slice(0, 30) ~ '...' : produto.descricao|striptags }}</p>
                        </div>
                        {% if is_granted('ROLE_USER') %}
                            <div class="card-action row valign-wrapper no-margin">
                                <span class="left col s6">Adicione ao Carrinho</span>
                                <form action="" type="POST" class="right-aligned col s3">
                                    <input id="input-{{ produto.id }}" class="input-altura" type="number">
                                </form>
                                <a onclick="addCart({{ produto.id }})" data-target="modal1" class="btn blue right-aligned col s2" data-id="{{ produto.id }}">
                                    <i class="material-icons">
                                        add_shopping_cart
                                    </i>
                                </a>
                            </div>
                        {% endif %}
                        {% if is_granted('ROLE_ADMIN') %}
                            <div class="card-action row valign-wrapper no-margin">
                                <span>Arquivar</span>
                                <a onclick="arquivar({{ produto.id }})" data-target="modal1" class="btn red right-aligned col s2" data-id="{{ produto.id }}">
                                    <i class="material-icons">
                                        remove_circle_outline
                                    </i>
                                </a>
                            </div>
                        {% endif %}
                    </div>
                </div>

        {% endfor %}



       {% if is_granted('ROLE_ADMIN') %}
            <div class="row">
                <div class="col s12">
                    <h3>Produtos Arquivados</h3>
                    <div class="row">
                        {% for produto in produtosArquivados  %}
                            <div class="col s12 m12 l4">
                                <div class="card">
                                    <div class="card-image">
                                        {% if produto.img %}
                                            <img class="imagem-card" src="/images/{{ produto.img }}">
                                        {% else %}
                                            {#<div class="imagem-card">#}
                                            <img class="imagem-card" src="data:image/jpeg;base64,">
                                            {#</div>#}
                                        {% endif %}
                                        <span class="card-title card-titulo">{{ produto.nome }} (R$ {{ produto.precoVenda }})</span>
                                    </div>
                                    <div class="card-content">
                                        <p>{{ produto.descricao|striptags|length > 30 ? produto.descricao|striptags|slice(0, 30) ~ '...' : produto.descricao|striptags }}</p>
                                    </div>
                                    <div class="card-action row valign-wrapper no-margin">
                                        <span>Desarquivar</span>
                                        <a onclick="arquivar({{ produto.id }})" data-target="modal1" class="btn green right-aligned col s2" data-id="{{ produto.id }}">
                                            <i class="material-icons">
                                                remove_circle_outline
                                            </i>
                                        </a>
                                    </div>
                                </div>
                            </div>

                        {% endfor %}
                    </div>
                </div>
            </div>
        {% endif %}

    <script>
        function arquivar(id) {
            var idProduto = id;
            let route = "{{ path('arquivar') }}";
            location.href = route + '/' + idProduto ;
        }

        function addCart(id) {
            var idProduto = id;
            var qtdProduto = document.getElementById('input-' + id).value;
            let route = "{{ path('adicionar_carrinho') }}";
            location.href = route + '/' + idProduto + '?qtdProduto=' + qtdProduto;
            }
    </script>

{% endblock %}


